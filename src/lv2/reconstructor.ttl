@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
<https://github.com/ctsexton/reconstructor-lv2>
        a lv2:Plugin ;
        lv2:project <https://github.com/ctsexton> ;
        doap:name "Reconstructor" ;
        doap:license <http://opensource.org/licenses/isc> ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
                a lv2:AudioPort ,
                        lv2:InputPort ;
                lv2:index 0 ;
                lv2:symbol "in" ;
                lv2:name "In"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 1 ;
                lv2:symbol "out" ;
                lv2:name "Out"
        ] , [
                a lv2:ControlPort ,
                        lv2:InputPort ;
                lv2:index 2 ;
                lv2:symbol "freeze" ;
                lv2:name "Freeze" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                lv2:portProperty lv2:integer ;
        ] , [
                a lv2:ControlPort ,
                        lv2:InputPort ;
                lv2:index 3 ;
                lv2:symbol "transpose" ;
                lv2:name "Transpose" ;
                lv2:default 0.0 ;
                lv2:minimum -2.0 ;
                lv2:maximum 2.0 ;
        ] , [
                a lv2:ControlPort ,
                        lv2:InputPort ;
                lv2:index 4 ;
                lv2:symbol "detune" ;
                lv2:name "Detune" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ] .
